<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:AnySort.ViewModel"
                    xmlns:view="clr-namespace:AnySort.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SortVM}">
        <view:Sort/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:FoundVM}">
        <view:Found/>
    </DataTemplate>

</ResourceDictionary>